let arr = ["1st", "2nd", "3rd", "4th"];
console.log(arr.length, arr);


arr.shift();
console.log("arr.shift();", arr.length, arr);


arr.pop();
console.log("arr.pop();", arr.length, arr);


arr.unshift("Farhana");
console.log("arr.unshift('Farhana');", arr.length, arr);


arr.push("Asha");
console.log("arr.push('Asha');", arr.length, arr);